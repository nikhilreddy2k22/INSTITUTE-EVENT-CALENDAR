          <div>
            <form action="/club_post" method="post">
                <h2 class="title">Add Club</h2>
                <div class="form">
                    <div class="form-group">
                        <label for="name">Club Name</label><br>
                        <input class="form-control" type="text" name="clubname" id="name"  required />
                    </div>
                    <div class="form-group">
                        <label for="email">Email</label><br>
                        <input class="form-control" type="email" name="email" id="email"   />
                    </div>
                    <div class="form-group">
                        <label class="lab" for="name">Incharge Name</label><br>
                        <input class="form-control" type="text" name="name" id="incharge"   />
                    </div>
                    <div class="ffp">
                        <button class="btn" type="submit">Submit</button>
                    </div>
                </div>
            </form>
          </div>
       </div>
       </div>
    </div>
   </section>
</body>
</html